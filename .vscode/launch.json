{
    "version": "0.2.0",
    "configurations": [
        
      {
        "name": "Debug main.cpp",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/main.exe", 
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": true, 
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "miDebuggerPath": "C:/TDM-GCC/bin/gdb.exe", 
        "preLaunchTask": "build",
        "logging": {
          "moduleLoad": false,
          "programOutput": true
        }
      }
    ]
  }
  